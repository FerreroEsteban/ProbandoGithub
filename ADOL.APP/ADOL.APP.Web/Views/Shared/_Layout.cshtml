<!DOCTYPE html>
<html class=" js flexbox canvas canvastext webgl no-touch geolocation postmessage websqldatabase indexeddb hashchange history draganddrop websockets rgba hsla multiplebgs backgroundsize borderimage borderradius boxshadow textshadow opacity cssanimations csscolumns cssgradients cssreflections csstransforms csstransforms3d csstransitions fontface generatedcontent video audio localstorage sessionstorage webworkers applicationcache svg inlinesvg smil svgclippaths" lang="es">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <title>@ViewBag.Title - Mi aplicación ASP.NET MVC</title>
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <script src="http://ajax.googleapis.com/ajax/libs/angularjs/1.2.26/angular.min.js"></script>
    <link rel="stylesheet" href="content/normalize.min.css">
    <link rel="stylesheet/less" type="text/css" href="content/main.less">
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link href="http://fonts.googleapis.com/css?family=Roboto:400,700,900" rel="stylesheet" type="text/css">
    <script src="scripts/vendor/modernizr-2.8.3-respond-1.4.2.min.js"></script>
    <script src="scripts/less.min.js" type="text/javascript"></script>
    <script src="~/Scripts/jquery-1.7.1.js"></script>
</head>
<body>
    @{

    }
    <div class="content" >
        @RenderBody()        
    </div>
    <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
    <script src="http://cdnjs.cloudflare.com/ajax/libs/velocity/0.2.1/jquery.velocity.min.js"></script>
    <script src="scripts/mtree.js"></script>
    <script src="scripts/autoNumeric/autoNumeric-1.9.25.js"></script>
    <script>
        jQuery('.tab a').on('click', function (e) {
            var currentAttrValue = jQuery(this).attr('href');

            // Show/Hide Tabs
            jQuery('.tab-content' + currentAttrValue).show().siblings().hide();

            // Change/remove current tab to active
            jQuery(this).addClass('selected').siblings().removeClass('selected');

            e.preventDefault();
        });
    </script>
    <script>
        jQuery('.delete-btn').on('click', function (e) {

            // Change/remove current tab to active
            jQuery(this).parent('.apuesta').remove();

            e.preventDefault();
        });
    </script>
    <script src="scripts/main.js"></script>
    
    @Scripts.Render("~/bundles/jquery")
    @RenderSection("scripts", required: false)
</body>
</html>
