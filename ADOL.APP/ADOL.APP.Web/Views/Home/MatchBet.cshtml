    /************* col-a menu **********/
    ul.mtree {
        opacity: 0;
        margin-left: 0;
    }

        ul.mtree ul {
            margin-left: 1em;
        }

        ul.mtree li {
            list-style: none;
        }

        ul.mtree a {
            display: block;
        }

        ul.mtree li.mtree-node > a {
            font-weight: bold;
        }

        ul.mtree a {
            -webkit-transition: all 300ms ease-out;
            -moz-transition: all 300ms ease-out;
            -ms-transition: all 300ms ease-out;
            -o-transition: all 300ms ease-out;
            transition: all 300ms ease-out;
        }

        ul.mtree.transit {
            background: #111;
            border-right: 1px solid #323232;
            border-bottom: 1px solid #323232;
        }

            ul.mtree.transit ul {
                margin-left: 0;
            }

                ul.mtree.transit ul > li {
                    font-size: .9em;
                }

            ul.mtree.transit li.mtree-node {
                position: relative;
            }

                ul.mtree.transit li.mtree-node > a:before {
                    color: #CCC;
                    font-weight: normal;
                    position: absolute;
                    right: 20px;
                }

            ul.mtree.transit li.mtree-open > a:before {
                content: '-';
            }

            ul.mtree.transit li.mtree-closed > a:before {
                content: '+';
            }

            ul.mtree.transit ul > li:first-child {
                margin-top: 1px;
            }

            ul.mtree.transit li.mtree-open > a {
                background: #222;
            }

            ul.mtree.transit li > a:hover,
            ul.mtree.transit li.mtree-active > a {
                background: #333;
                color: #f7b22c;
            }

            ul.mtree.transit li.mtree-node > ul > li:last-child {
                margin-bottom: .5em;
                padding-bottom: .5em;
                border-bottom: 1px solid #333;
            }

            ul.mtree.transit li.mtree-node:last-child > ul > li:last-child {
                margin-bottom: 0;
                padding-bottom: 0;
                border-bottom: none;
            }

            ul.mtree.transit a {
                padding: 5px 0px 5px 0.8em;
                color: #CCC;
                text-decoration: none;
            }

            ul.mtree.transit li li > a {
                padding-left: 1.6em;
            }

            ul.mtree.transit li li li > a {
                padding-left: 2.4em;
            }

            ul.mtree.transit li li li li > a {
                padding-left: 3.2em;
            }

            ul.mtree.transit li li li li li > a {
                padding-left: 4em;
            }
    /********** col-b breadcrum / event ************/
    .breadcrum {
        overflow: hidden;
        font-size: 0.8em;
        display: inline-block;
        border: 1px solid #222222;
        margin-bottom: 1em;
        -webkit-border-radius: 5px;
        -moz-border-radius: 5px;
        border-radius: 5px;
        -moz-background-clip: padding;
        -webkit-background-clip: padding-box;
        background-clip: padding-box;
    }

        .breadcrum a {
            -webkit-transition: all 0.3s linear;
            -moz-transition: all 0.3s linear;
            -o-transition: all 0.3s linear;
            -ms-transition: all 0.3s linear;
            transition: all 0.3s linear;
            padding: 0.5em 1em;
            display: inline-block;
            text-decoration: none;
        }

            .breadcrum a:nth-child(1) {
                background-color: rgba(255, 255, 255, 0.18);
            }

            .breadcrum a:nth-child(2) {
                background-color: rgba(255, 255, 255, 0.15);
            }

            .breadcrum a:nth-child(3) {
                background-color: rgba(255, 255, 255, 0.13);
            }

            .breadcrum a:nth-child(4) {
                background-color: rgba(255, 255, 255, 0.1);
            }

            .breadcrum a:hover {
                background-color: rgba(255, 255, 255, 0.09);
                text-decoration: underline;
            }
    /*********/
    .banner {
        display: block;
        margin-bottom: 0.5em;
    }

        .banner img {
            width: 100%;
            -webkit-border-radius: 5px;
            -moz-border-radius: 5px;
            border-radius: 5px;
            -moz-background-clip: padding;
            -webkit-background-clip: padding-box;
            background-clip: padding-box;
            overflow: hidden;
        }
    /*********/
    .event {
        background-color: #333333;
        -webkit-border-radius: 5px;
        -moz-border-radius: 5px;
        border-radius: 5px;
        -moz-background-clip: padding;
        -webkit-background-clip: padding-box;
        background-clip: padding-box;
        padding: 1em;
        margin-bottom: 1em;
        font-size: 1em;
        border: 1px solid #414141;
    }

    .event {
        -webkit-transition: all 0.3s linear;
        -moz-transition: all 0.3s linear;
        -o-transition: all 0.3s linear;
        -ms-transition: all 0.3s linear;
        transition: all 0.3s linear;
        opacity: 0.8;
    }

        .event:hover {
            opacity: 1;
        }

        .event .heading {
            display: table;
            width: 100%;
            padding: 0 0 0.5em 0;
        }

            .event .heading > span {
                display: table-cell;
            }

        .event .select-bet {
            width: 100%;
            font-size: 0.8em;
            border: 1px solid #212121;
            -webkit-border-radius: 3px;
            -moz-border-radius: 3px;
            border-radius: 3px;
            -moz-background-clip: padding;
            -webkit-background-clip: padding-box;
            background-clip: padding-box;
            overflow: hidden;
        }

            .event .select-bet .value {
                color: #f7b22c;
            }

            .event .select-bet div {
                overflow: hidden;
                text-align: left;
                cursor: pointer;
                padding: 0.5em 0.8em;
                background: #272727;
                float: left;
            }

                .event .select-bet div span:last-child {
                    float: right;
                }

                .event .select-bet div:hover {
                    background: #414141;
                }

            .event .select-bet .selected {
                background: #f7b22c;
            }

                .event .select-bet .selected .value {
                    color: white;
                }

    .select-bet.tres div {
        width: 33.333333333%;
    }

        .select-bet.tres div:nth-child(2) {
            border-left: 1px solid #212121;
            border-right: 1px solid #212121;
        }

    .select-bet.dos div {
        width: 50%;
    }

        .select-bet.dos div:nth-child(odd) {
            border-right: 1px solid #212121;
        }

    .sub-bet {
        border-top: 1px solid #383838;
        margin-bottom: 1.5em;
        margin-left: -0.3em;
        margin-right: -0.3em;
        padding: 0 0.3em;
    }

        .sub-bet h5 {
            font-size: 0.8em;
            padding: 1em 0 1em 0;
            text-align: left;
            color: #A3A3A3;
        }

    .event-date,
    .event-time {
        text-align: left;
        font-size: 0.7em;
        width: 10%;
    }

    .event-title {
        text-transform: uppercase;
        font-size: 0.8em;
        text-align: center;
        width: 60%;
    }

        .event-title .team1,
        .event-title .team2 {
            font-weight: 700;
        }

    .event-stats,
    .event-lines {
        text-align: right;
        font-size: 0.7em;
        width: 10%;
    }
    /********** col-c talon de apuestas ************/
    .delete-btn {
        -webkit-transition: all 0.3s linear;
        -moz-transition: all 0.3s linear;
        -o-transition: all 0.3s linear;
        -ms-transition: all 0.3s linear;
        transition: all 0.3s linear;
        display: inline-block;
        padding: 7px 9px;
        font-size: 10px;
        line-height: 9px;
        color: white;
        text-decoration: none;
        background: #2F2F27;
        -webkit-border-radius: 100%;
        -moz-border-radius: 100%;
        border-radius: 100%;
        -moz-background-clip: padding;
        -webkit-background-clip: padding-box;
        background-clip: padding-box;
        position: absolute;
        top: 12px;
        right: 10px;
        font-weight: 400;
        border: 1px solid #575757;
    }

        .delete-btn:hover {
            background: #575757;
            border: 1px solid #2F2F27;
        }

    #talon {
        font-size: 0.8em;
        line-height: 2em;
        padding: 1em;
        background: #111111;
        border: 1px solid #323232;
        margin-top: 2em;
        margin-right: 1em;
