        -moz-border-radius: 5px;
        border-radius: 5px;
        -moz-background-clip: padding;
        -webkit-background-clip: padding-box;
        background-clip: padding-box;
    }

        #talon h5 {
            font-size: 1em;
            text-align: center;
            text-transform: uppercase;
            font-weight: 700;
            border-bottom: 1px solid #323232;
            margin: 0 -1em 0em -1em;
            padding: 0 0.5em 0.5em 0.5em;
            color: #f7b22c;
        }

        #talon .yourcredit {
            font-size: 0.8em;
            padding: 0.2em 1em;
            background: #353535;
            -webkit-border-radius: 5px;
            -moz-border-radius: 5px;
            border-radius: 5px;
            -moz-background-clip: padding;
            -webkit-background-clip: padding-box;
            background-clip: padding-box;
        }

            #talon .yourcredit span {
                font-weight: 700;
                float: right;
            }

        #talon .tipodeapuesta {
            position: relative;
            height: 46px;
            z-index: 999;
            overflow: visible;
        }

            #talon .tipodeapuesta > span {
                float: left;
                font-weight: 700;
                padding: 1em 0;
            }

            #talon .tipodeapuesta .tab {
                float: right;
                -webkit-border-radius: 5px;
                -moz-border-radius: 5px;
                border-radius: 5px;
                -moz-background-clip: padding;
                -webkit-background-clip: padding-box;
                background-clip: padding-box;
                font-size: 0.8em;
                height: 50px;
            }

                #talon .tipodeapuesta .tab .selected {
                    color: #f7b22c;
                    background: transparent url(http://www.gonni.com.ar/betplatino/sports/css/selected.png) no-repeat center bottom;
                }

                #talon .tipodeapuesta .tab a {
                    display: inline-block;
                    padding: 1em;
                    vertical-align: middle;
                    cursor: pointer;
                    text-decoration: none;
                    color: white;
                    margin: 2px;
                }

                    #talon .tipodeapuesta .tab a:hover {
                        color: #f7b22c;
                    }

        #talon .value {
            float: right;
            color: #f7b22c;
            text-transform: uppercase;
        }

        #talon .apuesta {
            position: relative;
            border-top: 1px solid #323232;
            border-bottom: 1px solid #323232;
            padding: 1em;
        }

            #talon .apuesta .title {
                font-weight: 700;
                color: #f7b22c;
                font-size: 1.1em;
            }

            #talon .apuesta .desc {
                font-size: 0.8em;
                line-height: 1em;
                padding-bottom: 1em;
            }

            #talon .apuesta .cantidad .number {
                float: right;
                text-transform: uppercase;
            }

                #talon .apuesta .cantidad .number input {
                    width: 50px;
                    text-align: right;
                }

        #talon .resumen {
            border-bottom: 1px solid #323232;
            padding: 1em;
            margin: 0 -1em 1em;
        }

        #talon .confirmar {
            text-align: center;
        }

            #talon .confirmar .btn {
                display: block;
                margin-left: 0;
                margin-right: 0;
            }

    .simple,
    .combinada {
        display: none;
        margin: 0 -1em;
        border: 1px solid #f7b22c;
